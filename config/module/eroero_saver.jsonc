{
    /*
    *   指向图片SQLlite数据库的路径，若不存在会自动创建
    *   用`~`指代用户目录
    * @type: str
    */
    "db_path":"~/.config/",

    /*
    *   指向图片SQLlite数据库的路径，若不存在会自动创建
    *   用`~`指代用户目录
    * @type: str
    */
    "data_path":"~/data/cdfbot/eroero_saver/db/img.db",
    
    /*
    *   文件(图片)命名方案，自己按照Template string相关规范来写(PEP 292)。
    *   目前支持的命名占位符有:
    *   $md5  : MD5
    *   $hash : 图片P-Hash
    *   $ext  : File Extention
    * @type: str
    */
    "img_name_format":"${hash}.${ext}"
}